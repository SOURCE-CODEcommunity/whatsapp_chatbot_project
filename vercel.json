{
    "builds": [{
        "src": "whatsapp_chatbot/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.12"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "whatsapp_chatbot/wsgi.py"
        }
    ]
}